class Conference
{
	String name;
	ArrayList<Division> Divisions;
	public Conference(String name)
	{
		this.name = name;
	}
	public String print()
	{
		return "";
	}
}
class Division
{
	String name;
	ArrayList<Team> Teams;
	public Division(String name)
	{
		this.name = name;
	}
}
class Team
{
	String name;
	ArrayList<Team> Players;
	public Team(String name)
	{
		this.name = name;
	}
}
class Player
{
	String name;
	int score;
	public Player(String name,int score)
	{
		this.name = name;
		this.score = score;
	}
}

