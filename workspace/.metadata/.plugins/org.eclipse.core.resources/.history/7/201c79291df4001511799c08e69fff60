abstract class Animal {
	public Animal(){
		System.out.println("Constructeur abstract");
	}
	public void eat(Animal D){
		System.out.println("eat A");}
	}
class Dog extends Animal {
	public void eat2(Dog D) {
		System.out.println("eat D");
		}
}
class Main {
	public static void main(String[] args) {
		Animal a = new Dog();
		Dog b = new Dog();
	}
}